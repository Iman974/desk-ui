{
  "name": "desk-ui",
  "description": "HTML front-end for visualization (3D objects and medical images)",
  "author": "Sebastien Valette <sebastien.valette@creatis.insa-lyon.fr> (http://www.creatis.insa-lyon.fr/~valette)",
  "license": "CECILL-B",
  "version": "1.3.2",
  "repository": {
    "type": "git",
    "url": "git://github.com/valette/desk-ui.git"
  },
  "main": "build/index.html",
  "dependencies": {
    "fs-extra": "7.0.1",
    "shelljs": "0.8.3"
  },
  "devDependencies": {
    "ace-builds": "1.4.2",
    "async": "2.6.1",
    "bluebird": "3.5.3",
    "c3": "0.6.12",
    "chalk": "2.4.1",
    "css-loader": "2.1.0",
    "d3": "~5.7.0",
    "heap": "0.2.6",
    "jssha": "2.3.1",
    "jstat": "1.7.1",
    "kdt": "0.1.0",
    "lodash": "4.17.11",
    "numeric": "1.2.6",
    "operative": "0.4.6",
    "random-js": "1.0.8",
    "socket.io-client": "2.2.0",
    "style-loader": "0.23.1",
    "terser": "3.14.0",
    "three": "0.100.0",
    "ttest": "1.1.0",
    "uploadmgr": "git+https://github.com/johnspackman/UploadMgr.git",
    "url-loader": "1.1.2",
    "webpack": "4.28.3",
    "webpack-cli": "3.1.2",
    "webworkify-webpack": "2.1.3",
    "xterm": "3.9.1"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "build": "npm run clean && npm run qooxdoo && npm run bundle",
    "buildAddons": "node source/buildAddons.js",
    "bundle": "webpack && npm run terser",
    "clean": "rm -rf build && rm -rf api && rm -rf source/script",
    "qooxdoo": "python generate.py source && python generate.py build && python generate.py api",
    "terser": "terser source/script/bundle.js --mangle -o build/script/bundle.js",
    "updateAll": "npm run updateQooxdoo && rm -rf node_modules && git pull && npm install && npm run build",
    "updateQooxdoo": "cd qooxdoo && git pull && cd .."
  }
}
