{
  "name": "desk-ui",
  "description": "HTML front-end for visualization (3D objects and medical images)",
  "author": "Sebastien Valette <sebastien.valette@creatis.insa-lyon.fr> (http://www.creatis.insa-lyon.fr/~valette)",
  "license": "CECILL-B",
  "version": "2.3.0",
  "repository": {
    "type": "git",
    "url": "git://github.com/valette/desk-ui.git"
  },
  "main": "build/index.html",
  "dependencies": {
    "@babel/core": "^7.11.6",
    "fs-extra": "9.0.1",
    "shelljs": "0.8.4"
  },
  "devDependencies": {
    "@qooxdoo/compiler": "1.0.0-beta-20200904-1332",
    "@qooxdoo/framework": "^6.0.0-beta-20200904-1332",
    "ace-builds": "1.4.12",
    "async": "3.2.0",
    "bluebird": "3.7.2",
    "c3": "0.7.20",
    "chalk": "4.1.0",
    "chroma-js": "2.1.0",
    "css-loader": "4.3.0",
    "d3": "6.1.1",
    "heap": "0.2.6",
    "jssha": "3.1.2",
    "jstat": "1.9.3",
    "kdt": "0.1.0",
    "lodash": "4.17.20",
    "numeric": "1.2.6",
    "operative": "0.4.6",
    "papaya-viewer": "1.0.1449",
    "random-js": "2.1.0",
    "socket.io-client": "2.3.0",
    "style-loader": "1.2.1",
    "three": "0.120.1",
    "ttest": "1.1.0",
    "url-loader": "4.1.0",
    "webpack": "4.44.1",
    "webpack-cli": "3.3.12",
    "webworkify-webpack": "2.1.5",
    "xterm": "4.9.0"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "build": "npm run clean && npm run bundle && npm run qooxdoo",
    "buildAddons": "node source/buildAddons.js",
    "bundle": "node source/build-worker.js && webpack",
    "clean": "rm -rf compiled",
    "compile": "qx compile --target source && qx compile --target build",
    "deploy" : "npx qx deploy --clean --out dist",
    "qooxdoo": "qx pkg update && qx pkg install && npm run compile",
    "updateAll": "npm run updateQooxdoo && rm -rf node_modules && git pull && npm install && npm run build",
    "updateQooxdoo": "cd qooxdoo && git pull && cd .."
  }
}
